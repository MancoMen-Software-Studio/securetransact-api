{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "baseName": {
      "value": "securetransact"
    },
    "environment": {
      "value": "dev"
    },
    "containerImage": {
      "value": "PLACEHOLDER_REGISTRY/securetransact-api:latest"
    },
    "postgresConnectionString": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/SUBSCRIPTION_ID/resourceGroups/RESOURCE_GROUP/providers/Microsoft.KeyVault/vaults/KEYVAULT_NAME"
        },
        "secretName": "postgres-connection-string"
      }
    },
    "jwtSecretKey": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/SUBSCRIPTION_ID/resourceGroups/RESOURCE_GROUP/providers/Microsoft.KeyVault/vaults/KEYVAULT_NAME"
        },
        "secretName": "jwt-secret-key"
      }
    },
    "encryptionKey": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/SUBSCRIPTION_ID/resourceGroups/RESOURCE_GROUP/providers/Microsoft.KeyVault/vaults/KEYVAULT_NAME"
        },
        "secretName": "encryption-key"
      }
    },
    "hmacKey": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/SUBSCRIPTION_ID/resourceGroups/RESOURCE_GROUP/providers/Microsoft.KeyVault/vaults/KEYVAULT_NAME"
        },
        "secretName": "hmac-key"
      }
    }
  }
}
